<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.optimyth.com/schema/definitions/library_metadata"
         technology="swift" name="os" standard="swift">

  <description url="https://developer.apple.com/documentation/os">Logging tools</description>

  <module name="os">
    <function name="os_log" signature="os_log(StaticString, dso: UnsafeRawPointer, log: OSLog, type: OSLogType, CVarArg...)">
      <parameters>
        <param type="StaticString"/>
        <param type="UnsafeRawPointer" name="dso"/>
        <param type="OSLog" name="log"/>
        <param type="OSLogType" name="type"/>
        <param type="CVarArg"/>
      </parameters>
      <sink argpos="0, 1, 2" kind="log_forging" />
      <sink argpos="0, 1, 2" kind="privacy_violation"/>
    </function>

  </module>

</library>