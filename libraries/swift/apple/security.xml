<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.optimyth.com/schema/definitions/library_metadata"
         technology="swift" name="security" standard="swift">

  <description url="https://developer.apple.com/documentation/security">Security framework used to protect information, establish trust, and control access to software</description>

  <module name="Security">
    <module name="Trust" >
      <class name="SecTrust" />
      
      <function name="SecTrustEvaluate" signature="SecTrustEvaluate(_:_:)" >
        <tags>certificate_check</tags>
        <parameters>
          <param type="Security.Trust.SecTrust" />
          <param type="UnsafeMutablePointer" />
        </parameters>
      </function>
      <function name="SecKeyEncrypt" signature="SecKeyEncrypt(_:_:)" >
        <tags>encryption:4</tags>
      </function>
    </module>
  </module>

</library>