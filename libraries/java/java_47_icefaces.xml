<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.optimyth.com/schema/definitions/library_metadata"
         name="java.icefaces" standard="IceFaces" technology="java">

  <description url="http://www.icesoft.org/java/projects/ICEfaces/overview.jsf">Rich Internet Application Framework for Java</description>

  <!-- http://res.icesoft.org/docs/v4_latest/core/api/overview-summary.html -->

  <module name="org.icefaces.util" >
    <class name="org.icefaces.util.JavaScriptRunner" >
      <method name="runScript" signature="runScript(javax.faces.context.FacesContext,java.lang.String)" >
        <sink kind="code_injection" argpos="1" resource="web" />
      </method>
    </class>
  </module>

  <module name="org.icefaces.ace.util" >
    <class name="org.icefaces.ace.util.Utils" >
      <method name="getCookie" signature="getCookie(java.lang.String,javax.servlet.http.HttpServletRequest)" >
        <return type="javax.servlet.http.Cookie" />
      </method>
      <method name="getRequest" signature="getRequest(javax.faces.context.FacesContext)" >
        <return type="javax.servlet.http.HttpServletRequest" />
      </method>
    </class>
  </module>
</library>