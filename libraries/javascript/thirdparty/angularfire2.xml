<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.optimyth.com/schema/definitions/library_metadata"
         technology="javascript" name="angularfire2" standard="angularfire2">

  <description url="https://firebaseopensource.com/projects/angular/angularfire2/">Angular library for Google's Firebase platform</description>

  <module name="angularfire2" >
    <class name="AngularFirestore" >
      <function name="collection">
        <return type="AngularFirestoreCollection" />
        <source argpos="-1" kind="database_input"/>
        <sink argpos="1" kind="nosql_injection"/>
      </function>
      <function name="list">
        <return type="AngularFirestoreCollection" />
        <source argpos="-1" kind="database_input"/>
        <sink argpos="1" kind="nosql_injection"/>
      </function>
    </class>
  </module>

</library>