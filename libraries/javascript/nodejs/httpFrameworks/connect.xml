<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.optimyth.com/schema/definitions/library_metadata"
         name="nodejs/connect" standard="nodejs" technology="javascript">

  <module name="nodejs/connect" standard="connect">
    <description url="https://www.npmjs.com/package/connect">Connect server framework under Node.js</description>
    <!-- Yeah, Connect and Express 4.x do not extend each other, but for us this is enough -->
    <class name="nodejs/connect" supertypes="nodejs/express">
      <function name="#module#">
        <tags>server.new,app.new</tags>
        <return type="nodejs/express.Application"/>
      </function>
    </class>
  </module>
</library>