<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.optimyth.com/schema/definitions/library_metadata"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         name="selenium" technology="python">

  <description url="https://pypi.org/project/selenium/">Python language bindings for Selenium WebDriver</description>

  <module name="selenium">
    <module name="webdriver" >
      <function name="Firefox" signature="FireFox()" >
        <return type="selenium.webdriver" />
      </function>
      <function name="Chrome" signature="Chrome()" >
        <return type="selenium.webdriver" />
      </function>
      <function name="Remote" signature="Remote()" >
        <return type="selenium.webdriver" />
      </function>
      <function name="PhantomJS" signature="PhantomJS()" >
        <return type="selenium.webdriver" />
      </function>

      <function name="find_element_by_xpath" signature="find_element_by_xpath(xpath)" >
        <sink argpos="0" kind="xpath_injection" resource="other" />
      </function>
    </module>
  </module>
</library>