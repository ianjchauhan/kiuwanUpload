<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.optimyth.com/schema/definitions/library_metadata"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         name="Binary_Data_Services" technology="python">

  <description url="https://docs.python.org/3/library/binary.html">Binary Data Services (Python 3)</description>

  <module name="struct" standard="python3">
    <member name="error" type="Exception" />

    <function name="pack" signature="pack(fmt, v1, v2, ...)" >
      <return type="bytes" />
    </function>
    <function name="pack_into" signature="pack_into(fmt, buffer, offset, v1, v2, ...)">
      <return type="NoneType" />
    </function>
    <function name="unpack" signature="unpack(fmt, buffer)" >
      <return type="tuple" />
    </function>
    <function name="unpack_from" signature="unpack_from(fmt, buffer, offset=0)" >
      <return type="tuple" />
    </function>
    <function name="iter_unpack" signature="iter_unpack(fmt, buffer)" >
      <return type="generator" />
    </function>
    <function name="calcsize" signature="calcsize(fmt)" >
      <return type="int" />
    </function>

    <class name="Struct" >
      <function name="pack" signature="pack(v1, v2, ...)" >
        <return type="bytes" />
      </function>
      <function name="pack_into" signature="pack_into(buffer, offset, v1, v2, ...)">
        <return type="NoneType" />
      </function>
      <function name="unpack" signature="unpack(buffer)" >
        <return type="tuple" />
      </function>
      <function name="unpack_from" signature="unpack_from(buffer, offset=0)" >
        <return type="unpack_iterator" />
      </function>
      <function name="iter_unpack" signature="iter_unpack(buffer)" >
        <return type="generator" />
      </function>

      <field name="format" type="str" />
      <field name="size" type="int" />
    </class>
  </module>
</library>
