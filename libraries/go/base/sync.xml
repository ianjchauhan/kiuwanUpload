<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.optimyth.com/schema/definitions/library_metadata"
         technology="go" name="sync">


  <module name="sync">
    <description url="https://pkg.go.dev/sync">Synchronization primitives</description>

    <class name="sync.Mutex" >
      <method name="Lock" signature="Lock()" />
      <method name="Unlock" signature="Unlock()" />
    </class>
    <class name="sync.WaitGroup" >
      <method name="Add" signature="Add()" />
      <method name="Wait" signature="Wait()" />
      <method name="Done" signature="Done()" />
    </class>

    <function name="sync.Mutex" signature="sync.Mutex()" >
      <return type="sync.Mutex" />
    </function>
    <function name="sync.WaitGroup" signature="sync.WaitGroup()" >
      <return type="sync.WaitGroup" />
    </function>
  </module>
</library>