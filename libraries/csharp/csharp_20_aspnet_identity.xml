<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.optimyth.com/schema/definitions/library_metadata"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         name="Microsoft.AspNet.Identity" standard="aspnet.identity" technology="csharp">
  <description url="https://docs.microsoft.com/previous-versions/aspnet/dn253016(v=vs.108)">ASP.NET Identity</description>

  <class name="Microsoft.AspNet.Identity.Owin.SignInManagerExtensions">
    <method name="ExternalSignIn" signature="ExternalSignIn()" match="name">
      <sink argpos="-1" kind="improper_authentication" resource="web"/>
    </method>
    <method name="PasswordSignIn" signature="PasswordSignIn()" match="name">
      <sink argpos="-1" kind="improper_authentication" resource="web"/>
    </method>
    <method name="SignIn" signature="SignIn()" match="name">
      <sink argpos="-1" kind="improper_authentication" resource="web"/>
    </method>
    <method name="TwoFactorSignIn" signature="TwoFactorSignIn()" match="name">
      <sink argpos="-1" kind="improper_authentication" resource="web"/>
    </method>
  </class>
  
  <class name="Microsoft.AspNet.Identity.Owin.SignInManager">
    <method name="PasswordSignIn" signature="PasswordSignIn()" match="name">
      <sink argpos="-1" kind="improper_authentication" resource="web"/>
    </method>
    <method name="PasswordSignInAsync" signature="PasswordSignInAsync()" match="name">
      <sink argpos="-1" kind="improper_authentication" resource="web"/>
    </method>
    <method name="SignInAsync" signature="SignInAsync()" match="name">
      <sink argpos="-1" kind="improper_authentication" resource="web"/>
    </method>
    <method name="TwoFactorSignInAsync" signature="TwoFactorSignInAsync()" match="name">
      <sink argpos="-1" kind="improper_authentication" resource="web"/>
    </method>
  </class>

</library>